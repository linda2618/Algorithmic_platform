state的信息\r\n    store.commit('changeStyle', !store.state.showLoginView)\r\n}\r\n</script>\r\n\r\n<style lang=\"less\" scoped>\r\n.type {\r\n    display: none;\r\n}\r\n\r\n\r\n.el-header {\r\n    width: 1700px;\r\n    margin: 0 auto;\r\n    font-size: 18px;\r\n    height: 60px;\r\n    line-height: 30px;\r\n    background-color: #fff;\r\n\r\n    .header_left {\r\n        float: left;\r\n\r\n        .icon-a-shujutongji20beifen2:before {\r\n            font-size: 50px;\r\n        }\r\n    }\r\n\r\n    .header_right {\r\n        float: right;\r\n        margin-right: 10px;\r\n\r\n        .el-avatar {\r\n            margin-top: -6px;\r\n            --el-avatar-size: 42px;\r\n\r\n        }\r\n\r\n        .iconfont {\r\n            font-size: 32px;\r\n        }\r\n\r\n        // .header_right ul li:last-child {\r\n        //     line-height: 20px;\r\n        // }\r\n    }\r\n\r\n\r\n\r\n    ul>li {\r\n        list-style: none;\r\n        float: left;\r\n        font-style: 16px;\r\n        margin-right: 25px;\r\n\r\n        a {\r\n            color: #000;\r\n            text-decoration: none;\r\n        }\r\n    }\r\n}\r\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}